{
  "name": "react-s3-image-uploader",
  "version": "0.0.1",
  "description": "react + s3 + axios sample app",
  "main": "index.js",
  "scripts": {
    "start": "node main.js",
    "prebuild": "mkdirp public",
    "build": "browserify -t babelify index.js -o public/bundle.js",
    "watch": "watchify -t babelify index.js -o public/bundle.js -d",
    "test": "xo"
  },
  "author": "harukibun <harukibun@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.2.47",
    "axios": "^0.9.1",
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-dropzone": "^3.3.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint-config-xo": "^0.12.0",
    "eslint-config-xo-react-space": "0.1.0",
    "eslint-plugin-react": "^4.2.3",
    "watchify": "^3.7.0",
    "xo": "*"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "envs": [
      "node",
      "browser"
    ],
    "extends": "xo-react-space",
    "overrides": [
      {
        "files": "main.js",
        "esnext": false
      }
    ]
  }
}
